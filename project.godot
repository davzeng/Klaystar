[application]

config/name="Klay's World"
run/main_scene="res://scenes/hud/home.tscn"
config/icon="res://assets/imgs/icon.png"

[autoload]

Save="*res://scenes/save.gd"
MusicPlayer="*res://scenes/music_player.tscn"
HomeState="*res://scenes/hud/home_state.gd"

[debug]

settings/stdout/print_fps=true
gdscript/warnings/return_value_discarded=false

[display]

window/size/width=1920
window/size/height=1080
window/size/fullscreen=true
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[global]

vsync=false

